#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules/vue-demi/bin/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules/vue-demi/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/node_modules:/Users/terrymin/Desktop/person-file/node_modules:/Users/terrymin/Desktop/node_modules:/Users/terrymin/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules/vue-demi/bin/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules/vue-demi/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/vue-demi@0.13.1_vue@3.2.37/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules/.pnpm/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/vitepress-starter/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/ES-demo/node_modules:/Users/terrymin/Desktop/person-file/A_project-demo/node_modules:/Users/terrymin/Desktop/person-file/node_modules:/Users/terrymin/Desktop/node_modules:/Users/terrymin/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vue-demi-fix.js" "$@"
else
  exec node  "$basedir/../../bin/vue-demi-fix.js" "$@"
fi
